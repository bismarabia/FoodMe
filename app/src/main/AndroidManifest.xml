        </activity>
        <activity android:name=".dashboard.Dashboard"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".menu.RVMenu"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".cart.RVCart"
            android:theme="@style/AppTheme" />
        <activity
